{
    "peacock.color": "#86efac",
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "deno.enable": true,
    "deno.lint": true,
    "deno.suggest.imports.autoDiscover": true,
    "deno.codeLens.implementations": true,
    "deno.codeLens.referencesAllFunctions": true,
    "deno.codeLens.references": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.currentLine.enabled": false,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#b3f5cb",
        "activityBar.background": "#b3f5cb",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#b28ef0",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#b3f5cb",
        "statusBar.background": "#86efac",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#59e98d",
        "statusBarItem.remoteBackground": "#86efac",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#86efac",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#86efac99",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "terminal.integrated.enableImages": true,
    "terminal.integrated.defaultProfile.windows": "Command Prompt"
}